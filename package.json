{
  "name": "snowboy",
  "version": "1.4.2",
  "description": "Snowboy is a customizable hotword detection engine",
  "main": "index.js",
  "typings": "lib/node/index.d.ts",
  "binary": {
    "module_name": "snowboy",
    "module_path": "./lib/node/binding/{configuration}/{node_abi}-{platform}-{arch}/",
    "remote_path": "./{module_name}/v{version}/{configuration}/",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz",
    "host": "https://github.com/bugusounet/snowboy"
  },
  "scripts": {
    "install": "scripts/installer.sh",
    "postinstall": "tsc -p tsconfig.json",
    "rebuild": "./node_modules/.bin/electron-rebuild"
  },
  "author": [
    "KITT.AI <snowboy@kitt.ai>",
    "bugsounet <github@bugsnana.fr>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bugsounet/snowboy/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bugsounet/snowboy"
  },
  "dependencies": {
    "electron-rebuild": "^1.10.1",
    "node-pre-gyp": "^0.6.39",
    "path": "^0.12.7",
    "child_process": "^1.0.2"
  },
  "devDependencies": {
    "@types/node": "^6.14.9",
    "nan": "^2.14.0",
    "typescript": "^2.9.2"
  }
}
